  <div class="section padding_layout_1 Shopping_cart_section">
    <div class="container">
      <div class="row">
        <div class="col-sm-12 col-md-12">
          <div class="product-table">
            <table class="table">
              <thead>
                <tr>
                  <th></th>
                  <th>SẢN PHẨM</th>
                  <th>SỐ LƯỢNG</th>
                  <th class="text-center">GIÁ</th>
                  <th class="text-center">TỔNG</th>
                  <th> </th>
                  <th> </th>
                </tr>
              </thead>
              <% for(let i=0 ; i < dataNameLK.length ; i ++) { %>
                <tr>
                  <td>
                    <input a type="checkbox" onclick="myfun('<%=dataNameLK[i].recordset[0].malk%>')" id="check"name="fav_language" <% if(dataNameLK[i].recordset[0].checkStore==1 ) { %><%= checked %><% } %>>
                  </td>
                  <td class="col-sm-8 col-md-6">
                    <div class="media">
                      <a class="thumbnail pull-left" href="#"> <img class="media-object"
                          src="/linhkien/<%=  dataNameLK[i].recordset[0].Hinh1 %>" alt="#"></a>
                      <div class="media-body">
                        <h5 class="media-heading"><a href="#">
                            <%= dataNameLK[i].recordset[0].TenLK %>
                          </a></h5>
                        <span>Status: </span><span class="text-success">In Stock<span id="cardID"></span></span>
                      </div>
                    </div>
                  </td>
                  <td class="col-sm-1 col-md-1 text-center" style="text-align: center">
                    <input class="price_table" type="number" value="<%= dataNameLK[i].recordset[0].soluong %>">
                  </td>
                  <td class="col-sm-1 col-md-1 text-center">
                    <p class="price_table">
                      <%= dataNameLK[i].recordset[0].Gia %>đ
                    </p>
                  </td>
                  <td class="col-sm-1 col-md-1 text-center">
                    <p class="price_table">
                      <%= dataNameLK[i].recordset[0].soluong * dataNameLK[i].recordset[0].Gia %>đ
                    </p>
                  </td>
                  <td class="col-sm-1 col-md-1">
                    <form action="/SuaItem?malk=<%= dataNameLK[i].recordset[0].malk %>" method="post">
                      <input type="submit" id="btn_sua" value="sửa" class="bt_main">
                    </form>
                  </td>
                  <td class="col-sm-1 col-md-1">
                    <form action="/XoaItem?malk=<%= dataNameLK[i].recordset[0].malk %>" method="post">
                      <input type="submit" id="btn_xoa" value="xóa" class="bt_main">
                    </form>
                  </td>
                </tr>
                <% } %>
                  </tbody>
            </table>

          </div>
          <div class="shopping-cart-cart">
            <table>
              <tbody>
                <tr class="head-table">
                  <td>
                    <h5>Giỏ Hàng</h5>
                  </td>
                  <td class="text-right">
                    <%= countItem1 %>
                  </td>
                </tr>
                <tr>
                  <td>
                    <h3>Tổng cộng</h3>
                  </td>
                  <td class="text-right">
                    <h4>
                      <%= monney %> đ
                    </h4>
                  </td>
                </tr>
                <form action="/payPage" method="get">
                  <tr>
                    <td><a href="~/ITNEXT/index"><button type="button" class="button">Tiếp tục mua sắm</button></a></td>
                    <td><input type="submit" class="button" id="btn_ThanhToan" value="Thanh Toán" /></td>
                  </tr>
                </form>
              </tbody>
            </table>
          </div>
